{
	"name": "asarsyntaxhighlighter",
	"displayName": "AsarSyntaxHighlighter",
	"description": "",
	"version": "0.0.2",
	"engines": {
		"vscode": "^1.48.0"
	},
	"categories": [
		"Programming Languages"
	],
	"contributes": {
		"grammars": [
			{
				"language": "assembly",
				"scopeName": "source.asar",
				"path": "./syntaxes/asar.tmLanguage"
			},
			{
				"scopeName": "source.asm.mnemonics",
				"path": "./syntaxes/mnemonics.tmLanguage"
			},
		],
		"languages": [
			{
				"id": "assembly",
				"extensions": [
					".asm"
				],
				"configuration": "./language-configuration.json"
			}
		]
	}
}
