{
	"name": "asarsyntaxhighlighter",
	"displayName": "AsarSyntaxHighlighter",
	"description": "Highlighter for Asar, a SNES assembler with support for 65c816, SPC700 and GSU mnemonics.",
	"version": "1.0.0",
	"engines": {
		"vscode": "^1.48.0"
	},
	"categories": [
		"Programming Languages"
	],
	"contributes": {
		"grammars": [
			{
				"language": "assembly",
				"scopeName": "source.asar",
				"path": "./syntaxes/asar.tmLanguage"
			},
			{
				"path": "./syntaxes/mnemonics.tmLanguage",
				"scopeName": "source.asm.mnemonics"
			}
		],
		"languages": [
			{
				"id": "assembly",
				"extensions": [
					".asm"
				],
				"configuration": "./language-configuration.json"
			}
		]
	}
}
