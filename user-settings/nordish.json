{
    "editor.tokenColorCustomizations": 
    {
        "textMateRules": [
            {
                "name": "Asar comments",
                "scope": ["comment.asar.line"],
                "settings": {
                    "foreground": "#4f596d"
                    }
            },
            {
                "name": "Asar strings",
                "scope": ["string.asar"],
                "settings": {
                    "foreground": "#A3BE8C"
                    }
            },


            {
                "name": "65c816 Mnemonics",
                "scope": ["keyword.asar.mnemonics.65c816"],
                "settings": {
                    "foreground": "#81A1C1"
                    }
            },
            {
                "name": "65c816 Mnemonics - Length",
                "scope": ["keyword.asar.mnemonics.65c816.length"],
                "settings": {
                    "foreground": "#81A1C1"
                    }
            },
            {
                "name": "65c816 Mnemonics - Status",
                "scope": ["keyword.asar.mnemonics.65c816.return"],
                "settings": {
                    "foreground": "#bb75a9"
                    }
            },
            {
                "name": "65c816 Mnemonics - Status (REP/SEP)",
                "scope": ["keyword.asar.mnemonics.65c816.status.repsep"],
                "settings": {
                    "foreground": "#e0959a"
                    }
            },
            {
                "name": "65c816 Mnemonics - Indexes",
                "scope": ["keyword.asar.indexes"],
                "settings": {
                    "foreground": "#81A1C1"
                    }
            },
            {
                "name": "65c816 Mnemonics - Breakpoints",
                "scope": ["keyword.asar.mnemonics.65c816.breakpoint"],
                "settings": {
                    "foreground": "#d47171"
                    }
            },
            {
                "name": "65c816 Mnemonics - Stack operations",
                "scope": ["keyword.asar.mnemonics.65c816.stack"],
                "settings": {
                    "foreground": "#c6c783"
                    }
            },



            
            {
                "name": "Asar constants",
                "scope": ["constant.asar.numeric"],
                "settings": {
                    "foreground": "#D8DEE9"
                    }
            },
            {
                "name": "Asar decimal constant",
                "scope": ["constant.asar.numeric.decvalue"],
                "settings": {
                    "foreground": "#D8DEE9"
                    }
            },
            {
                "name": "Asar binary constant",
                "scope": ["constant.asar.numeric.binvalue"],
                "settings": {
                    "foreground": "#D8DEE9"
                    }
            },




            {
                "name": "Asar binary directives",
                "scope": "keyword.asar.binary",
                "settings": {
                    "foreground": "#81A1C1",
                    "fontStyle": "italic"
                }
            },
            {
                "name": "Asar binary raw data",
                "scope": "keyword.asar.binary.raw",
                "settings": {
                    "foreground": "#81A1C1",
                    "fontStyle": ""
                }
            },
            {
                "name": "Asar include source",
                "scope": "keyword.asar.include",
                "settings": {
                    "foreground": "#81A1C1",
                    "fontStyle": "italic"
                }
            },
            

            
            {
