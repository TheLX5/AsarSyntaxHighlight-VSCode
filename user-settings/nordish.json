                "scope": ["string.asar"],
                "settings": {
                    "foreground": "#A3BE8C"
                    }
            },


            {
                "name": "65c816 Mnemonics",
                "scope": ["keyword.asar.mnemonics"],
                "settings": {
                    "foreground": "#81A1C1"
                    }
            },
            {
                "name": "65c816 Mnemonics - Length",
                "scope": ["keyword.asar.mnemonics.length"],
                "settings": {
                    "foreground": "#66809b"
                    }
            },
            {
                "name": "65c816 Mnemonics - Status",
                "scope": ["keyword.asar.mnemonics.return"],
                "settings": {
                    "foreground": "#a579be"
                    }
            },
            {
                "name": "65c816 Mnemonics - Status (REP/SEP)",
